<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="tz.cn.dao.CustomerDao">
	
	<!-- 根据id查询用户信息 -->
	<select id="findCustomerById" resultType="TbCustomer">
